// type of database
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

// client generator
generator client {
  provider = "prisma-client-js"
}

// Models
model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  email     String   @unique
  name      String?
  password  String

  @@map("users")
}
